{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
        "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
        {
            "Name": "Console"
        }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "https://localhost:5001"
      }
    }
  }
}
